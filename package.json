{
  "name": "codelab",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "prod": "npm run prod-database & npm run prod-server",
    "prod-server": "nodemon ./dist/index.js --watch ./dist/version --delay 1.5 ./config.yml",
    "prod-database": "mongod --auth --bind_ip 127.0.0.1 --port 31287 --noscripting --dbpath /srv/data/codelab",
    "dev": "npm run database & npm run dev-server & npm run watch",
    "dev-server": "nodemon ./dist/index.js --watch dist/ --ignore 'dist/workbench.js' --ignore 'dist/resources/' --ignore 'dist/views/' --delay 1.5 -- ./config.dev.yml",
    "dev-lessons": "./scripts/babel-node.sh -- ./scripts/compile-lesson.js",
    "database": "mongod --dbpath ./.data/codelab/",
    "watch": "./node_modules/.bin/gulp watch",
    "workbench": "./scripts/babel-node.sh -- ./src/workbench.js ./config.dev.yml",
    "dev-workbench": "nodemon -w ./dist -x \"npm run workbench\" --delay 1.5",
    "clean": "rm -rf ./dist",
    "compile": "./node_modules/.bin/gulp compile",
    "compile-lesson": "./scripts/babel-node.sh -- ./lessons/compiler/index.js",
    "deploy": "./scripts/deploy.sh",
    "deploy-lessons": "./scripts/deploy-lessons.sh",
    "deploy-all": "npm run deploy-lessons && npm run deploy",
    "marker": "electron ./lessons/marker/index.html",
    "create-lesson": "./scripts/new-lesson.sh",
    "test": "jest",
    "dev-tests": "nodemon -x npm run test"
  },
  "author": "",
  "license": "ISC",
